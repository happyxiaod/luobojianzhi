body{position: relative;padding-bottom:50px;}
section{margin-top:15px;}
section .sec_wrap{overflow:hidden;margin-bottom:10px;}
.sec_wrap div{background-color: #fff;padding:5px 3%;margin-bottom:1px;}
.sec_wrap b{margin-right:5px;}
.sec_wrap s{display:inline-block;margin-right:5px;color:#337ab7;margin-top:5px;}
section span{display:block;margin-bottom:5px;}
section span a{float:right;color:#eb7312;}
section span a:hover{color:#eb7312;}
.wrap_icon{background:url(../images/icon.png) no-repeat 3px center;padding-left:25px;}
section ul li{float:left;width:20%;}
section ul li img{width:90%;}
.discuss li{border-bottom:1px solid #efeff0;min-height:70px;width:100%;padding:10px 0;}
.discuss li:last-child{border-bottom:0;}
.discuss img{width:15%;float:left;}
.discuss div{float:left;width:82%;padding:0;padding-left:3%;margin:0;overflow:hidden;}
.discuss .con{overflow:hidden;float:none;padding:0;margin:0;width:100%;}
.discuss div h5{line-height:22px;padding:0;}
.discuss div b{color:#666;float:left;margin-right:0;}
.discuss div strong{color:#666;float:right;display:block;}
.discuss p{margin-top:10px;display:block;}

.sec_wrap .discuss_con{width:100%;height:185px;padding:10px 3%;box-sizing:border-box;}
.discuss_con textarea{padding:5px;width:100%;height:120px;border:1px solid #ccc;margin-bottom:10px;box-sizing:border-box;-webkit-appearance:none;}
.discuss_con input{padding:5px 10px;float:right;background-color: #eb7312;color:#fff;}


.footer_as{width:100%;height:45px;line-height:45px;border-top:1px solid #eb7312;background-color: #fff;position:fixed;bottom:0;left:0;}
.footer_as li{float:left;border-right:1px solid #eb7312;width:33.33%;box-sizing:border-box;float:left;}
.footer_as li a{color:#eb7312;text-align:center;display:block;}
.footer_as li b{margin-right:5px;}
.footer_as li:last-child{background-color: #fff;border-right:0;}

