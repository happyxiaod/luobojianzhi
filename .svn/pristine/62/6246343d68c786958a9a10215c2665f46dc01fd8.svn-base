<script>
    $(document).ready(function() {
        bindClick();

        //在账号密码输入框按回车，就触发登陆按钮的click事件
        $("#usernameInput, #passwordInput").keydown(function(e) {
            if(e.keyCode==13){
                $("#loginBtn").click();
            }
        });
    });
</script>