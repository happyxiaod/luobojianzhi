body{background-color: #f4d6b2;}
section{padding:0 3%;}
.title{color:#7f2d00;}
h2{display:block;height:50px;line-height:50px;font-weight:bold;text-align: center;font-size:18px;}
.title p{line-height:24px;position:relative;}
.title b{background:url(../images/weal.png) no-repeat 0 0;width:19px;height:24px;position: absolute;bottom:2px;}
.title b:nth-child(2){margin-left:30px;}

.main{margin-top:30px;}
.main div{margin-bottom:15px;position:relative;}
.main p{position:relative;height:30px;line-height:30px;margin-bottom:5px;}
.main b{background: url(../images/weal.png) no-repeat 0 -75px;width:30px;height:30px;position: absolute;bottom:2px;margin-left:5px;}
.main input{width:100%;height:40px;line-height:40px;padding:0 5px;box-sizing:border-box;background-color: #fff;border-radius:5px;}
.main div:nth-child(2) b{width:30px;height:30px;background-position: 0 -157px;}
.main div:nth-child(2)>b{background:none;right:10px;bottom:5px;}
.main div:nth-child(4) b{width:29px;height:27px;background-position: 0 -314px;}
.main div .spinner{background-color: #fff;border:0;border-radius:5px;}
.main div:nth-child(4) div{/* background-color: #fff; */margin-bottom:10px;}
.main div:nth-child(5) b{width:27px;height:27px;background-position: 0 -391px;}
.main div:nth-child(6) b{width:24px;height:30px;background-position: 0 -468px;}
.main input[type=submit]{-webkit-appearance:none;background-color: #eb7312;color:#fff;cursor:pointer;font-size:18px;margin-top:20px;}
.main input[type=date]{-webkit-appearance:none;}

/* 日历 */
.cdrBody .dayOver{background-color: #f90;border:0;color:#fff;}
.cdrHeader,#cdrMenu .cdrMenu td{font-family:"宋体";}
.cdrHeader .headerOver{border:0;border-top:1px solid #fff;}
#cdrMenu .cdrMenu{border:1px solid #eb7312;}

.con{width:100%;height:100%;min-height:638px;position:relative;}
.con img{width:100%;position:absolute;z-index:-1;}
.con .download{width:190px;height:150px;margin:0 auto;position:absolute;z-index: 1;left:25%;bottom:80px;}
.download span{display:block;text-align:center;margin-bottom:20px;}
.download>div{width:100%;height:50px;background-color:#7f2d00;line-height: 50px;border-radius:5px;margin-bottom:10px;}
.download a{display:block;text-align: center;color:#fff;width:100%;box-sizing:border-box;padding-left:15px;}
.download a{background:url(../images/flat.png) no-repeat 15px 10px;display:inline-block;}
.download div:nth-child(3) a{height:40px;background-position: 15px -70px;}