@CHARSET "utf-8";

/* 头 */
.top-line{width:100%;height:3px;background-color:#ed7612;}
.container{width:1200px;height:120px;margin:0 auto;background-color:#fff;}
.ablock{/* box-shadow: 0 2px 10px #e4e4e4; */height:120px;position:relative;}
.ablock1{box-shadow: 0 2px 10px #e4e4e4; }
.ablock2{box-shadow: 0 1px 5px #c3c3c3; }
h1{float:left;}
h1 a{background: url(../images/logo.png) no-repeat;width: 244px;height:76px;display: block;margin:37px 27px 8px 0;}
.nav{margin-top:85px;float:left;}
.nav li{float:left;margin:0 19px;}
.nav li a{color:#333;font-size:18px;}
.nav li a:hover{color:#ed7612;}
.nav li.current a{color:#ed7612;}
.pull-left {float: left!important;}
.pull-right {float: right!important;}
.header-nav {color: white;background: #f90;height: 30px;line-height:30px;padding-left:2px;}
.login-before{padding:0 3px;}
.login-before li{float:right;padding:0 3px;font-size:14px;position:relative;}
.login-before li ul{position:absolute;top:30px;z-index:99;width:120px;right:-3px;background-color:#f90;padding:5px 0;display:none;}
.login-before li ul li{height:30px;line-height:30px;display:block;width:80px;padding:0;background:url(../images/user_icon.png) no-repeat 0 -423px;padding-left:25px;margin-left:25px;}
.login-before li ul li:nth-child(2){background-position:0 -460px;}
.login-before li ul li:nth-child(3){background-position:0 -494px;}
.login-before li ul li:nth-child(4){background-position:0 -528px;}
.login-before li ul li:nth-child(5){background-position:0 -558px;}
.login-before a {font-size: 16px;color: white;}
.login-before .nav-dlmt {float: right;height: 16px;width: 2px;margin-left: 8px;margin-right: 8px;background: #e6e1db;margin-top: 8px;}
.login-after{margin-top:76px;font-size:16px;}
.login-after li{float:right;padding:0 15px;border:1px solid #ed7612;margin-left:10px;}
.chose-city {font-weight: 600;font-size: 16px;position:absolute;left:80px;top:40px;}
.change-city {font-size: 14px;border: 1px solid #c9c9c9;}

.header-pt{float:left;margin-top:72px;}

/* 搜索 */
#search{width:100%;height:64px;background-color:#ed7612;margin-top:18px;}
.search{width:1200px;height:64px;margin:0 auto;}
.search_in{width:490px;height:38px;padding-top:13px;float:left;margin-left:290px;}
.search_in .BC_input_focus {border-color: #fff;position: relative;z-index: 2;width: 338px;height: 28px;line-height: 24px;padding: 5px 10px 5px 40px;border-top-left-radius:5px;border-bottom-left-radius:5px;float:left;font-size:14px;font-family:"微软雅黑";font-size:16px;background:#fff url(../images/search_icon.png) no-repeat 10px center;}
.search_in .BC_validation{width:90px;height:38px;border:2px solid #fff;background-color: #ed7612;border-left:0;color:#fff;border-top-right-radius:5px;border-bottom-right-radius:5px;font-size:18px;line-height:32px;float:left;font-family:"微软雅黑";cursor:pointer;}
.search_hot{height:65px;line-height:65px;float:left;}
.search_hot ul li{float:left;margin:0 12px;}
.search_hot ul li a{color:#fff;}

/* 尾部 */
#footer{width:100%;height:180px;background-color:#ed7612;margin-top:30px;}/*#ed7612 #f78015 #f6851f*/
.footer{width:1200px;margin:0 auto;}
.footer .line{width:2px;height:140px;background-color: #fff;opacity:0.5;float:left;margin-top:20px;}
.footer dl{float:left;width:298px;height:140px;margin-top:20px;color:#fff;text-align:center;}
.footer dl:last-child{margin-top:55px;}
.footer dl dt{font-size:18px;font-weight:bold;line-height:40px;}
.footer dl dd{line-height:34px;}
.footer dl dd a{color:#fff;}


.clearfix:after { content:"."; display:block; height:0; visibility:hidden; clear:both; }
.clearfix { zoom:1; }
.clearit { clear:both; height:0; font-size:0; overflow:hidden; }
textarea{resize:none;outline:none;color:#666;}

/* 下拉列表 */
.input-group {position: relative;display:table;border-collapse: separate;margin-bottom:0;}
.spinner {margin-right: 20px;float: left;height: 40px;border: 1px solid #ccc;}
.spinner-content {padding: 0 15px;min-width: 60px;display: table-cell;line-height:40px;}
.glyphicon {position: relative;top: 1px;display: inline-block;font-style: normal;font-weight: 400;line-height: 1;-webkit-font-smoothing: antialiased;-moz-osx-font-smoothing: grayscale;}
.input-group .form-control, .input-group-addon, .input-group-btn {display: table-cell;}
.input-group-addon, .input-group-btn {width: 1%;/* white-space: nowrap; */vertical-align: middle;}
.input-group-addon {padding: 6px 12px;font-size: 14px;font-weight: 400;line-height: 1;color: #555;text-align: center;background-color: #eee;border: 1px solid #ccc;border-radius: 4px;}
.spinner .glyphicon-triangle-bottom {width:auto;border: none;padding: 0;font-size:25px;color: #FF9A22;background: transparent;height:auto;cursor:pointer;margin-top:-4px;margin-right:10px;}
.demo-icon,.glyphicon
    {
      font-family: "fontello";
      font-style: normal;
      font-weight: normal;
      speak: none;
     
      display: inline-block;
      text-decoration: inherit;
      width:20px;
      text-align: center;
      font-variant: normal;
      text-transform: none;
      -webkit-font-smoothing: antialiased;
      -moz-osx-font-smoothing: grayscale;
    }
.dropdown-menu {position: absolute;top: 100%;left:-2px;z-index: 1000; display: none;float: left;min-width:160px;padding: 5px 0;margin: 2px 0 0;font-size: 14px;text-align: left;background-color: #fff;-webkit-background-clip: padding-box;background-clip: padding-box;border: 1px solid #ccc;border: 1px solid rgba(0,0,0,.15);border-radius: 4px;-webkit-box-shadow: 0 6px 12px rgba(0,0,0,.175);box-shadow: 0 6px 12px rgba(0,0,0,.175);cursor:pointer;max-height:120px;overflow-y:auto;}
.dropdown-menu li{line-height:22px;padding-left:5px;}
.wrong{margin-left:180px;margin-bottom:10px;color:#f00;background:url(../images/icon_wrong.png) no-repeat 0 center;padding-left:20px;line-height:20px;}

/* 分页 */
.page{overflow:hidden;height:30px;}
.page ul{float:right;}
.page li{float:left;border:1px solid #ccc;/* height:28px;width:28px;line-height:28px; */text-align:center;margin:0 5px;}
.page li.page_prev,.page li.page_next{width:62px;}
.page li.page_prev{margin-right:15px;margin-left:0;}
.page li.page_next{margin-left:15px;margin-right:0;}
.page li.cur a{background-color:#ed7612;color:#fff;}
.page li a{display:block;padding:3px 7px;}
.page li a:hover{background-color:#ed7612;color:#fff;}

.text-right {text-align: right;}
.text-center {text-align: center;}
.toolbar {
  position: fixed;
  right: 3%;
  bottom: 21%;
   }

.toolbar-iterm {
  position: relative;
  display: block;
  width: 52px;
  height: 52px;
  background-image: url(../images/toolbar.png);
  background-repeat: no-repeat;
  margin-top: 1px;
  transition: background-position 1s; }
  .toolbar-iterm:hover .toolbar-layer {
    opacity: 1;
    filter: alpha(opacity=100);
    transform: scale(1); }

.toolbar-iterm-weixin {
  background-position: 0 -798px; }
  .toolbar-iterm-weixin:hover {
    background-position: 0 -860px; }
  .toolbar-iterm-weixin .toolbar-layer {
    height: 212px;
    background-position: 0 0; }

.toolbar-iterm-feedback {
  background-position: 0 -426px; }
  .toolbar-iterm-feedback:hover {
    background-position: 0 -488px; }

.toolbar-iterm-app {
  background-position: 0 -550px; }
  .toolbar-iterm-app:hover {
    background-position: 0 -612px; }
  .toolbar-iterm-app .toolbar-layer {
    height: 194px;
    background-position: 0 -222px; }

.toolbar-iterm-top {
  background-position: 0 -674px; }
  .toolbar-iterm-top:hover {
    background-position: 0 -736px; }

.toolbar-layer {
  position: absolute;
  right: 46px;
  bottom: -10px;
  width: 172px;
  background-image: url(../images/toolbar.png);
  background-repeat: no-repeat;
  opacity: 0;
  filter: alpha(opacity=0);
  transform: scale(0.01);
  transform-origin: 95% 95%;
  transition: all 1s; }
